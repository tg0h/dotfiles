# pr
alias hc.="hc -f" # create pr but prefill title and body
alias hc,="hc -t " # create pr with title
alias hl="gh pr list"
alias hv="gh pr view --web" # pull request - view pr after creating it
alias ht="gh pr status"

# repo
alias hr="gh repo"
alias hr.="gh repo view --web"

alias hrc="gh repo create"
alias hrcp="gh repo create --public"
alias hrcpp="gh repo create --private"
alias hrl="gh repo list"
alias hro="gh repo clone"
alias hrd="gh repo delete"
