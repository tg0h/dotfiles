{
  "title": "Capslock RMeh",
  "rules": [
    {
      "description": "CapsLock to Right Meh/Escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_shift",
              "modifiers": ["right_control", "right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
