{
  "title": "Right Cmd to LHyper",
  "rules": [
    {
      "description": "Right Cmd to Left Hyper",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": ["left_control", "left_option", "left_command"]
            }
          ]
        }
      ]
    }
  ]
}
